#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/sink/source/urwbench/node_modules/.pnpm/assemblyscript@0.27.27/node_modules/assemblyscript/bin/node_modules:/home/sink/source/urwbench/node_modules/.pnpm/assemblyscript@0.27.27/node_modules/assemblyscript/node_modules:/home/sink/source/urwbench/node_modules/.pnpm/assemblyscript@0.27.27/node_modules:/home/sink/source/urwbench/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/sink/source/urwbench/node_modules/.pnpm/assemblyscript@0.27.27/node_modules/assemblyscript/bin/node_modules:/home/sink/source/urwbench/node_modules/.pnpm/assemblyscript@0.27.27/node_modules/assemblyscript/node_modules:/home/sink/source/urwbench/node_modules/.pnpm/assemblyscript@0.27.27/node_modules:/home/sink/source/urwbench/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/asc.js" "$@"
else
  exec node  "$basedir/../../bin/asc.js" "$@"
fi
