#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/sink/source/urwbench/node_modules/.pnpm/binaryen@116.0.0-nightly.20240114/node_modules/binaryen/bin/node_modules:/home/sink/source/urwbench/node_modules/.pnpm/binaryen@116.0.0-nightly.20240114/node_modules/binaryen/node_modules:/home/sink/source/urwbench/node_modules/.pnpm/binaryen@116.0.0-nightly.20240114/node_modules:/home/sink/source/urwbench/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/sink/source/urwbench/node_modules/.pnpm/binaryen@116.0.0-nightly.20240114/node_modules/binaryen/bin/node_modules:/home/sink/source/urwbench/node_modules/.pnpm/binaryen@116.0.0-nightly.20240114/node_modules/binaryen/node_modules:/home/sink/source/urwbench/node_modules/.pnpm/binaryen@116.0.0-nightly.20240114/node_modules:/home/sink/source/urwbench/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/wasm-opt" "$@"
else
  exec node  "$basedir/../../bin/wasm-opt" "$@"
fi
